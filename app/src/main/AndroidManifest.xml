<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.manas.smartshopping">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homepage" />
        <activity android:name=".orderactivity" />
        <activity android:name=".searchactivity" />
        <activity android:name=".myordersactivity" />
        <activity android:name=".myaddressesactivity" />
        <activity android:name=".wishlistactivity" />
        <activity android:name=".myaccountactivity" />
        <activity android:name=".tempactivity"></activity>
    </application>

</manifest>